/* sidconfig.h (template) */

/* @FOO@ : Define or undefine value FOO as appropriate. */

#ifndef _sidconfig_h_
#define _sidconfig_h_

/* Define if your processor stores words with the most significant
   byte first (like Motorola and SPARC, unlike Intel and VAX).  */
/* SID_WORDS_BIGENDIAN, SID_WORDS_LITTLEENDIAN  */
#define @SID_WORDS_ENDIANESS@

#define SID_SIZEOF_CHAR @SID_SIZEOF_CHAR@
#define SID_SIZEOF_SHORT_INT @SID_SIZEOF_SHORT_INT@
#define SID_SIZEOF_INT @SID_SIZEOF_INT@
#define SID_SIZEOF_LONG_INT @SID_SIZEOF_LONG_INT@

/* Define if your C++ compiler implements exception-handling.  */
@SID_HAVE_EXCEPTIONS@

/* Define if your compiler supports type ``bool''.
   If not, a user-defined signed integral type will be used.  */
@SID_HAVE_BOOL@

/* Define if you support file names longer than 14 characters.  */
@SID_HAVE_LONG_FILE_NAMES@

/* Define if you have the <strstrea.h> header file.  */
@SID_HAVE_STRSTREA_H@

/* Define if you have the strncasecmp function.  */
@SID_HAVE_STRNCASECMP@

/* Define if you have the strcasecmp function.  */
@SID_HAVE_STRCASECMP@

/* Define if standard member ``ios::binary'' is called ``ios::bin''. */
@SID_HAVE_IOS_BIN@

/* Define if standard member function ``fstream::is_open()'' is not available.  */
#undef SID_DONT_HAVE_IS_OPEN

/* Define whether istream member function ``seekg(streamoff,seekdir).offset()''
   should be used instead of standard ``seekg(streamoff,seekdir); tellg()''.
*/
#undef SID_HAVE_SEEKG_OFFSET

/* Defines to indicate that resid is in an non standard location.
   SID_HAVE_LOCAL_RESID indicates that an include path has been used
   to where the resid directory is located but does not include
   the resid directory name.
   SID_HAVE_USER_RESID indicates that a fully qualified include path
   has been provided that includes the resid directory name and therefore
   resids include files are available by calling them directly.
*/
@SID_HAVE_LOCAL_RESID@
@SID_HAVE_USER_RESID@

#endif /* _sidconfig_h_ */
